        flex-shrink: 0;
        padding: 0 2rem;
        display: flex;
        align-items: center;
        justify-content: center;
        color: rgba(156, 163, 175, 0.6);
        transition: color 0.3s ease;
    }
    
    .tech-logo:hover {
        color: rgba(212, 175, 55, 0.8);
    }
    
    @keyframes scroll {
        0% {
            transform: translateX(0);
        }
        100% {
            transform: translateX(-100%);
        }
    }
</style> 